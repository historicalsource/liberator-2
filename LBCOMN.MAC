	.MACRO	COMP		;COMPLIMENT ACCUMULATOR
	CLC
	EOR I,0FF
	ADC I,1
	.ENDM

	.MACRO SAVEXY		;SAVE X AND Y REGS ON STACK
	TXA
	PHA
	TYA
	PHA
	.ENDM

	.MACRO RESTXY		;RESTORE X AND Y FROM STACK
	PLA
	TAY
	PLA
	TAX
	.ENDM

	.MACRO PHX		;PUSH X ON STACK
	TXA
	PHA
	.ENDM

	.MACRO PHY		;PUSH Y ON STACK
	TYA
	PHA
	.ENDM

	.MACRO PLX		;PULL X FROM STACK
	PLA
	TAX
	.ENDM

	.MACRO PLY		;PULL Y FROM STACK
	PLA
	TAY
	.ENDM

	.MACRO POINT COLOR
	LDA BITDAT
	EOR I,COLOR
	STA BITDAT
	.ENDM

	.MACRO FENCE NUM
	.REPT NUM
	.BYTE 255
	.ENDR
	.ENDM

	.MACRO STATE ADDR,CODE	;GENERATE STATE CODES
	CODE	=.-SETUP
	.WORD	ADDR-1
	.GLOBL	CODE
	.ENDM

	.MACRO STATST ADDR,CODE	;GENERATE TEST STATES
	CODE	=.-TESTS
	.WORD	ADDR-1
	.GLOBL	CODE
	.ENDM

	.MACRO OVERF		;RECURSIVE RLE GENERATOR
	Z=LEVEL!0F
	.BYTE Z
	COUNT	=COUNT-0F
	.IF GT,COUNT-0F
	OVERF
	.ENDC
	.ENDM
	.PAGE
	.MACRO BYT A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,A13,A14,A15,A16
	SWITCH=0
	.IRP X,<A1,A2,A3,A4,A5,A6,A7,A8,A9,A10,A11,A12,A13,A14,A15,A16>
	.IF B,X
	.REXIT
	.ENDC
	.IF EQ,SWITCH
	COUNT=X
	SWITCH=1
	.IFF
	.IF NE,COUNT
	.IF EQ,X
	LEVEL=0
	.ENDC
	.IF EQ,X-1
	LEVEL=40
	.ENDC
	.IF EQ,X-2
	LEVEL=80
	.ENDC
	.IF EQ,X-3
	LEVEL=0C0
	.ENDC
	.IF EQ,X-4
	LEVEL=20
	.ENDC
	.IF EQ,X-5
	LEVEL=60
	.ENDC
	.IF EQ,X-6
	LEVEL=0A0
	.ENDC
	.IF EQ,X-7
	LEVEL=0E0
	.ENDC
	.IF GT,X-7
	.ERROR C;COVERSION ERROR ON LEVEL
	LEVEL=0
	.ENDC
	.IF GT,COUNT-0F
	OVERF
	.ENDC
	Z=LEVEL!COUNT
	.BYTE Z
	.IFF
	.IF EQ,X
	.=.-1
	Z=Z!10
	.BYTE Z
	.IFF
	.BYTE 0
	.REXIT
	.ENDC
	.IFTF
	SWITCH=0
	.ENDC
	.ENDC
	.ENDR
	.ENDM
